Sub Stock_Script():'------------------------------------------------------------'Make the headersCells(1, 9).Value = "Ticker"Cells(1, 10).Value = "Yearly Change"Cells(1, 11).Value = "Percent Change"Cells(1, 12).Value = "Total Stalk Volume"Cells(1, 16).Value = "TIcker"Cells(1, 17).Value = "Value"'------------------------------------------------------------'Find the begining row number of each stock in an Array called Array()Dim arr() As VariantDim row_length As Longrow_length = Cells(Rows.Count, "A").End(xlUp).RowDim num As Longnum = 0For i = 1 To row_length    If Cells(i + 1, 1).Value = Cells(i, 1).Value Then        Else        ReDim Preserve arr(num)        arr(num) = i + 1        num = num + 1    End IfNext i'------------------------------------------------------------'Print the Ticker, Yearly Change, Percent Change values and Total Stalk VolumeFor i = 0 To num - 2Cells(i + 2, 9).Value = Cells(arr(i), 1).Value  'TickerDim Yearly_Change As DoubleYearly_Change = Cells(arr(i + 1) - 1, 6).Value - Cells(arr(i), 3).Value 'Yearly ChangeCells(i + 2, 10).Value = Yearly_Change    If Cells(arr(i), 3).Value = 0 Then        Cells(i + 2, 11).Value = 0    Else        Cells(i + 2, 11).Value = Yearly_Change / Cells(arr(i), 3).Value      'Percent Change    End IfCells(i + 2, 11).NumberFormat = "0.00%"Cells(i + 2, 12).Value = Application.Sum(Range(Cells(arr(i), 7), Cells(arr(i + 1) - 1, 7))) 'Total Stalk VolumeNext i'------------------------------------------------------------'Coloring CellsFor i = 0 To num - 2    If Cells(i + 2, 10) >= 0 Then        Cells(i + 2, 10).Interior.Color = RGB(0, 255, 0)    Else        Cells(i + 2, 10).Interior.Color = RGB(255, 0, 0)    End If Cells(i + 2, 10).Font.Color = RGB(0, 0, 0) Next i '------------------------------------------------------------'Find Greatest % Increase, Greatest % Decrease and Greatest Total VolumeCells(2, 15).Value = "Greatest % Increase"Cells(3, 15).Value = "Greatest % Decrease"Cells(4, 15).Value = "Greatest Total Volume"Dim Increase As DoubleDim Increase_Ticker As StringDim Decrease As DoubleDim Decrease_Ticker As StringDim Volume As DoubleDim Volume_Ticker As StringIncrease = 0Increase_Ticker = “”Decrease = 0Decrease_Ticker = “”Volume = 0Volume_Ticker = “”For i = 1 To num - 1    If Cells(i + 1, 11) > Increase Then        Increase = Cells(i + 1, 11).Value        Increase_Ticker = Cells(i + 1, 9).Value    Else    End If    If Cells(i + 1, 11) < Decrease Then        Decrease = Cells(i + 1, 11).Value        Decrease_Ticker = Cells(i + 1, 9).Value    Else    End If    If Cells(i + 1, 12) > Volume Then        Volume = Cells(i + 1, 12).Value        Volume_Ticker = Cells(i + 1, 9).Value    Else    End IfNext iCells(2, 16).Value = Increase_TickerCells(2, 17).Value = IncreaseCells(2, 17).NumberFormat = "0.00%"Cells(3, 16).Value = Decrease_TickerCells(3, 17).Value = DecreaseCells(3, 17).NumberFormat = "0.00%"Cells(4, 16).Value = Volume_TickerCells(4, 17).Value = Volume '------------------------------------------------------------End Sub